@model IEnumerable<Resto.Data.Models.Restaurant>
@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <div>
        There are @Model.Count() restaurants in the DB.
    </div>
</div>

<ul>
    @foreach (var rest in Model)
    {
        <li>@rest.Name</li>
    }
</ul>